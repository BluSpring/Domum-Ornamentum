{
    "schemaVersion": 1,
    "id": "domum_ornamentum",
    "version": "${version}",
    "name": "Domum Ornamentum",
    "description": "Home decorations made easy. Out of several materials",
    "authors": [
        "LDT Team",
        "OrionDevelopment",
        "BluSpring"
    ],
    "contact": {
        "repo": "https://github.com/BluSpring/domum-ornamentum"
    },
    "license": "LGPL-3.0-only",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.ldtteam.domumornamentum.DomumOrnamentum"
        ],
        "client": [
            "com.ldtteam.domumornamentum.client.DomumOrnamentumClient"
        ],
        "fabric-datagen": [
            "com.ldtteam.domumornamentum.datagen.DomumDatagen"
        ]
    },
    "mixins": [
        "domum_ornamentum.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric-api": ">=${fabric_version}",
        "minecraft": "${minecraft_version}",
        "architectury": ">=${architectury_version}"
    },
    "accessWidener": "domum_ornamentum.accesswidener"
}